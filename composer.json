{
    "name": "samuelbie/mpesa",
    "type": "library",
    "description": "This is an interface to comunicate with Mpesa Open API Mozambique",
    "keywords": [
        "Laravel", "OPEN-API","Mpesa SDK", "Mpesa", "M-Pesa",  "c2b",  "b2c",  "b2b",  "reversal",  "query", "PHP"
    ],
    "homepage": "https://samuel-bie.github.io",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Samuel Bie",
            "email": "samuelbie@outlook.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "samuelbie\\mpesa\\": "src"
        },
        "files": [
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "samuelbie\\mpesa\\MpesaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
